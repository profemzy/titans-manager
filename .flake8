[flake8]
max-line-length = 106
extend-ignore = E203, W503, E402
exclude = 
    .git,
    __pycache__,
    build,
    dist,
    *.pyc,
    *.egg-info,
    .eggs,
    migrations,
    .env,
    .venv,
    env,
    venv,
    ENV,
    staticfiles,
    media
per-file-ignores =
    # imported but unused in __init__.py files
    */__init__.py: F401
max-complexity = 10
statistics = True
count = True